{
    "name": "ubient/laravel-flash-message",
    "description": "A Laravel package to display flash messages to your users in a fluent, clean, testable and easy-to-use way!",
    "keywords": [
        "ubient",
        "laravel-flash-message",
        "laravel",
        "flash",
        "message",
        "alert",
        "notification"
    ],
    "homepage": "https://github.com/ubient/laravel-flash-message",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Dekker",
            "email": "claudio@ubient.net",
            "homepage": "https://ubient.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/http": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "sebastian/diff": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ubient\\FlashMessage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ubient\\FlashMessage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ubient\\FlashMessage\\FlashMessageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
