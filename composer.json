{
    "name": "ubient/laravel-flash-message",
    "description": "A Laravel package to display flash messages to your users in a fluent, clean, testable and easy-to-use way!",
    "keywords": [
        "ubient",
        "laravel-flash-message",
        "laravel",
        "flash",
        "message",
        "alert",
        "notification"
    ],
    "homepage": "https://github.com/ubient/laravel-flash-message",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Dekker",
            "email": "claudio@ubient.net",
            "homepage": "https://ubient.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "laravel/framework": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ubient\\FlashMessage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ubient\\FlashMessage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ubient\\FlashMessage\\FlashMessageServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
